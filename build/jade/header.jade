nav.navbar.navbar-default.navbar-fixed-top
  div.container
    div.navbar-header
      button.navbar-toggle.collapsed(type="button", data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar")
        span.sr-only="Toogle navigation"
        each _ in Array(3)
          span.icon-bar
      a.navbar-brand(href="#")
        img.logoImage(src="./app/img/prodigiousLogo.png")
    div#navbar.navbar-collapse.collapse
      ul.nav.navbar-nav
        each _,i in Array(3)
          li.hvr-shutter-in-vertical
            a(id="Carousel"+(i+1) href="#" div="Carousel"+(i+1))
              | Carousel Type #{i+1}
      ul.nav.navbar-nav.navbar-right
        li.active
          a(href="#")="Luis Taylor"
